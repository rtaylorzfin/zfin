{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "array",
    "items": {
        "type": "object",
        "required": ["title", "accession", "details", "type"],
        "properties": {
            "title": {
                "type": "string"
            },
            "accession": {
                "type": "string"
            },
            "details": {
                "type": "string"
            },
            "type": {
                "type": "string",
                "enum": ["LOAD", "ERROR"]
            }
        }
    }
}
